.wf-alert-info { border-left: 3px solid var(--wf-primary); }
.wf-alert-success { border-left: 3px solid var(--wf-success); }
.wf-alert-warning { border-left: 3px solid var(--wf-warning); }
.wf-alert-danger { border-left: 3px solid var(--wf-danger); }

.wf-alert-close {
  margin-left: auto;
  background: transparent;
  border: none;
  font-size: 18px;
  cursor: pointer;
  opacity: 0.5;
}

.wf-alert-close:hover {
  opacity: 1;
}

.wf-toast {
  padding: 12px 16px;
  background: var(--wf-fg);
  color: var(--wf-bg);
  border-radius: var(--wf-radius);
  box-shadow: var(--wf-shadow-lg);
  font-size: 14px;
}

.wf-progress {
  height: 8px;
  background: var(--wf-border);
  border-radius: 4px;
  overflow: hidden;
}

.wf-progress-bar {
  height: 100%;
  background: var(--wf-fg);
  border-radius: 4px;
  transition: width 0.3s ease;
}

.wf-progress-indeterminate .wf-progress-bar {
  width: 30%;
  animation: wf-progress-indeterminate 1.5s ease-in-out infinite;
}

@keyframes wf-progress-indeterminate {
  0% { transform: translateX(-100%); }
  100% { transform: translateX(400%); }
}

.wf-progress-label {
  display: block;
  margin-bottom: 4px;
  font-size: 12px;
  color: var(--wf-muted);
}

.wf-spinner {
  display: inline-block;
  width: 20px;
  height: 20px;
  border: 2px solid var(--wf-border);
  border-top-color: var(--wf-fg);
  border-radius: 50%;
  animation: wf-spin 0.6s linear infinite;
}

.wf-spinner-xs { width: 12px; height: 12px; border-width: 2px; }
.wf-spinner-sm { width: 16px; height: 16px; border-width: 2px; }
.wf-spinner-md { width: 24px; height: 24px; border-width: 2px; }
.wf-spinner-lg { width: 32px; height: 32px; border-width: 3px; }
.wf-spinner-xl { width: 48px; height: 48px; border-width: 4px; }

@keyframes wf-spin {
  to { transform: rotate(360deg); }
}

/* Overlay Components */
.wf-tooltip-wrapper {
