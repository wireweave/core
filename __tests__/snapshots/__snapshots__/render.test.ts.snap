  border-radius: var(--wf-radius);
}

.wf-accordion-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  padding: 12px 16px;
  background: transparent;
  border: none;
  border-bottom: 1px solid var(--wf-border);
  font-size: 16px;
  font-weight: 500;
  cursor: pointer;
  text-align: left;
}

.wf-accordion-header:hover {
  background: rgba(0, 0, 0, 0.02);
}

.wf-accordion-content {
  padding: 16px;
}

/* Text Components */
.wf-text {
  margin: 0;
  line-height: 1.5;
}

.wf-text-xs { font-size: 12px; }
.wf-text-sm { font-size: 14px; }
.wf-text-base { font-size: 16px; }
.wf-text-md { font-size: 16px; }
.wf-text-lg { font-size: 18px; }
.wf-text-xl { font-size: 20px; }
.wf-text-2xl { font-size: 24px; }
.wf-text-3xl { font-size: 30px; }

.wf-text-normal { font-weight: 400; }
.wf-text-medium { font-weight: 500; }
.wf-text-semibold { font-weight: 600; }
.wf-text-bold { font-weight: 700; }

.wf-text-left { text-align: left; }
.wf-text-center { text-align: center; }
.wf-text-right { text-align: right; }
.wf-text-justify { text-align: justify; }

.wf-text-muted { color: var(--wf-muted); }

.wf-title {
  margin: 0 0 8px 0;
  font-weight: 600;
  line-height: 1.25;
}

/* Remove bottom margin when title is in a row (inline with other elements) */
.wf-row .wf-title {
  margin-bottom: 0;
}

h1.wf-title { font-size: 36px; }
h2.wf-title { font-size: 30px; }
h3.wf-title { font-size: 24px; }
h4.wf-title { font-size: 20px; }
h5.wf-title { font-size: 18px; }
h6.wf-title { font-size: 16px; }

.wf-link {
  color: var(--wf-fg);
  text-decoration: underline;
  cursor: pointer;
}

.wf-link:hover {
  opacity: 0.7;
}

/* Input Components */
.wf-form-field {
  margin-bottom: 16px;
}

.wf-input-label {
  display: block;
  margin-bottom: 4px;
  font-size: 14px;
  font-weight: 500;
}

.wf-input,
.wf-textarea,
.wf-select {
  display: block;
  width: 100%;
  padding: 8px 12px;
  border: 1px solid var(--wf-border);
  border-radius: var(--wf-radius);
  font-family: inherit;
  font-size: 14px;
  background: var(--wf-bg);
  color: var(--wf-fg);
  transition: border-color 0.15s ease;
}

/* Inputs in flex rows should not take full width */
.wf-row > .wf-input,
.wf-row > .wf-select {
  width: auto;
  flex: 1 1 auto;
  min-width: 120px;
}

.wf-input:focus,
.wf-textarea:focus,
