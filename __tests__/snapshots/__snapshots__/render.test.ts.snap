/* Base Styles */
.wf-page {
  font-family: var(--wf-font);
  color: var(--wf-fg);
  background: var(--wf-bg);
  box-sizing: border-box;
  position: relative;
  display: flex;
  flex-direction: column;
  /* Wireframe boundary visualization */
  box-shadow: 0 4px 24px rgba(0, 0, 0, 0.15), 0 0 0 1px rgba(0, 0, 0, 0.1);
  border-radius: 8px;
  overflow: hidden;
}

/* Col direct child of page should fill page height */
.wf-page > .wf-col {
  flex: 1;
  min-height: 0;
}

/* Row containing sidebar should fill remaining space */
.wf-page > .wf-row:has(.wf-sidebar),
.wf-page > .wf-row:has(.wf-main),
.wf-page > .wf-col > .wf-row:has(.wf-sidebar),
.wf-page > .wf-col > .wf-row:has(.wf-main) {
  flex: 1;
  min-height: 0;
  align-items: stretch;
}

.wf-page *, .wf-page *::before, .wf-page *::after {
  box-sizing: inherit;
}

/* Page with flex layout - enables justify/align for centering content */
.wf-page.wf-flex,
.wf-page[class*="wf-justify-"],
.wf-page[class*="wf-align-"] {
  display: flex;
  flex-direction: column;
}

/* Page centered content (both horizontal and vertical) */
.wf-page.wf-page-centered {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

/* Prevent direct children from stretching in centered pages */
.wf-page.wf-page-centered > * {
  flex: 0 0 auto;
}

/* Grid System - Explicit Gap Only */
.wf-row {
  display: flex;
  flex-wrap: nowrap;
}

.wf-col {
  display: flex;
  flex-direction: column;
  flex: 1 0 0%;
  box-sizing: border-box;
}

/* When explicit width is set, don't flex-grow */
.wf-row[style*="width:"],
.wf-col[style*="width:"] {
  flex: 0 0 auto;
}

.wf-col-1 { flex: 1 0 0%; min-width: 0; }
.wf-col-2 { flex: 2 0 0%; min-width: 0; }
.wf-col-3 { flex: 3 0 0%; min-width: 0; }
.wf-col-4 { flex: 4 0 0%; min-width: 0; }
.wf-col-5 { flex: 5 0 0%; min-width: 0; }
.wf-col-6 { flex: 6 0 0%; min-width: 0; }
.wf-col-7 { flex: 7 0 0%; min-width: 0; }
.wf-col-8 { flex: 8 0 0%; min-width: 0; }
.wf-col-9 { flex: 9 0 0%; min-width: 0; }
.wf-col-10 { flex: 10 0 0%; min-width: 0; }
.wf-col-11 { flex: 11 0 0%; min-width: 0; }
.wf-col-12 { flex: 12 0 0%; min-width: 0; }


