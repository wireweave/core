  color: var(--wf-bg);
  font-size: 12px;
  border-radius: var(--wf-radius);
  white-space: nowrap;
  z-index: 1000;
  pointer-events: none;
}

.wf-tooltip-top {
  bottom: 100%;
  left: 50%;
  transform: translateX(-50%);
  margin-bottom: 8px;
}

.wf-tooltip-bottom {
  top: 100%;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 8px;
}

.wf-tooltip-left {
  right: 100%;
  top: 50%;
  transform: translateY(-50%);
  margin-right: 8px;
}

.wf-tooltip-right {
  left: 100%;
  top: 50%;
  transform: translateY(-50%);
  margin-left: 8px;
}

.wf-popover {
  background: var(--wf-bg);
