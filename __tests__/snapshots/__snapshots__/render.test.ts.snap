svg.wf-icon-md { width: 20px; height: 20px; }
svg.wf-icon-lg { width: 24px; height: 24px; }
svg.wf-icon-xl { width: 32px; height: 32px; }

.wf-icon svg {
  display: block;
}

.wf-icon-wrapper {
  display: inline-flex;
  align-items: center;
  justify-content: center;
}

/* Data Components */
.wf-table {
  width: 100%;
  border-collapse: collapse;
  border: 1px solid var(--wf-border);
  font-size: 14px;
}

.wf-table th,
.wf-table td {
  padding: 10px 12px;
  text-align: left;
  border-bottom: 1px solid var(--wf-border);
}

.wf-table th {
  background: rgba(0, 0, 0, 0.02);
  font-weight: 600;
}

.wf-table-bordered th,
.wf-table-bordered td {
  border: 1px solid var(--wf-border);
}

.wf-table-striped tbody tr:nth-child(odd) {
  background: rgba(0, 0, 0, 0.02);
}

.wf-table-hover tbody tr:hover {
  background: rgba(0, 0, 0, 0.04);
}

.wf-list {
