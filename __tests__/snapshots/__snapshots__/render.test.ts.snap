}

.wf-progress-label {
  display: block;
  margin-bottom: 4px;
  font-size: 12px;
  color: var(--wf-muted);
}

.wf-spinner {
  display: inline-block;
  width: 20px;
  height: 20px;
  border: 2px solid var(--wf-border);
  border-top-color: var(--wf-fg);
  border-radius: 50%;
  animation: wf-spin 0.6s linear infinite;
