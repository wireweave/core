  margin-bottom: 8px;
}

.wf-tooltip-bottom {
  top: 100%;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 8px;
}

.wf-tooltip-left {
  right: 100%;
  top: 50%;
  transform: translateY(-50%);
