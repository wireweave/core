}

.wf-drawer-left {
  top: 0;
  left: 0;
  bottom: 0;
  width: 280px;
  border-right: 1px solid var(--wf-border);
}

.wf-drawer-right {
  top: 0;
  right: 0;
  bottom: 0;
  width: 280px;
  border-left: 1px solid var(--wf-border);
}

.wf-drawer-top {
  top: 0;
  left: 0;
  right: 0;
  height: auto;
  max-height: 50vh;
  border-bottom: 1px solid var(--wf-border);
}

.wf-drawer-bottom {
