.wf-spinner-xs { width: 12px; height: 12px; border-width: 2px; }
.wf-spinner-sm { width: 16px; height: 16px; border-width: 2px; }
.wf-spinner-md { width: 24px; height: 24px; border-width: 2px; }
.wf-spinner-lg { width: 32px; height: 32px; border-width: 3px; }
.wf-spinner-xl { width: 48px; height: 48px; border-width: 4px; }

