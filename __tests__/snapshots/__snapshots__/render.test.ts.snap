  flex-direction: column;
  border: 1px solid var(--wf-border);
  border-radius: var(--wf-radius);
  background: var(--wf-bg);
  padding: 16px;
}

/* Cards in flex rows: respect explicit width, shrink if needed */
.wf-row > .wf-card {
  flex: 0 1 auto;
  min-width: 0;
}

/* Cards without explicit width should expand to fill space */
.wf-row > .wf-card-flex {
  flex: 1 1 0%;
}

.wf-card-title {
  margin: 0 0 12px 0;
  font-size: 18px;
  font-weight: 600;
}

.wf-card-shadow-sm { box-shadow: var(--wf-shadow-sm); }
.wf-card-shadow-md { box-shadow: var(--wf-shadow-md); }
.wf-card-shadow-lg { box-shadow: var(--wf-shadow-lg); }
.wf-card-shadow-xl { box-shadow: var(--wf-shadow-xl); }

.wf-modal-backdrop {
  position: absolute;
  inset: 0;
