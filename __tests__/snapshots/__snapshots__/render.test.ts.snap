.wf-tabs {
  border-bottom: 1px solid var(--wf-border);
}

.wf-tab-list {
  display: flex;
  gap: 0;
  margin-bottom: -1px;
}

.wf-tab {
  padding: 10px 16px;
  background: transparent;
  border: none;
  border-bottom: 2px solid transparent;
  font-size: 14px;
  color: var(--wf-fg);
  cursor: pointer;
  transition: all 0.15s ease;
}

.wf-tab:hover {
  background: rgba(128, 128, 128, 0.1);
}

.wf-tab-active {
  border-bottom-color: var(--wf-fg);
  font-weight: 500;
}

.wf-breadcrumb {
  display: flex;
  align-items: center;
  font-size: 14px;
}

.wf-breadcrumb-item {
  color: var(--wf-fg);
  text-decoration: none;
}

.wf-breadcrumb-item:hover {
  text-decoration: underline;
}

.wf-breadcrumb-item[aria-current="page"] {
  color: var(--wf-muted);
}

.wf-breadcrumb-separator {
  margin: 0 8px;
  color: var(--wf-muted);
}

/* Semantic Markers */

/* Avatar marker - small circle placeholder */
.wf-semantic-avatar {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  background: var(--wf-muted);
  color: var(--wf-bg);
  font-size: 10px;
  font-weight: 600;
  vertical-align: middle;
  margin-right: 8px;
}

.wf-semantic-avatar-xs { width: 16px; height: 16px; }
.wf-semantic-avatar-sm { width: 24px; height: 24px; }
.wf-semantic-avatar-md { width: 32px; height: 32px; }
.wf-semantic-avatar-lg { width: 40px; height: 40px; }

/* Badge marker - inline status indicator (wireframe style: black/white only) */
.wf-semantic-badge {
  display: inline-block;
  padding: 2px 8px;
  font-size: 11px;
  font-weight: 500;
  border-radius: 9999px;
  vertical-align: middle;
  background: var(--wf-fg);
  color: var(--wf-bg);
}

/* Dot marker - status dot (wireframe style: black/gray only) */
.wf-semantic-dot {
  display: inline-block;
  width: 8px;
  height: 8px;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 6px;
  background: var(--wf-fg);
}

/* Inactive/default state uses gray */
.wf-semantic-dot-default { background: var(--wf-muted); }

/* Icon marker placeholder */
.wf-semantic-icon {
  display: inline-block;
  font-size: 12px;
  color: var(--wf-muted);
  vertical-align: middle;
}

/* Unknown marker - fallback */
.wf-semantic-unknown {
  display: inline-block;
  font-size: 11px;
  color: var(--wf-muted);
  font-style: italic;
}

/* Avatar + text layout in table cells */
.wf-cell-avatar-layout {
  display: flex;
  align-items: center;
  gap: 10px;
}

.wf-cell-avatar-text {
  display: flex;
  flex-direction: column;
  line-height: 1.4;
}

.wf-cell-avatar-text span:first-child {
  font-weight: 500;
}

.wf-cell-avatar-text span:last-child:not(:first-child) {
  font-size: 12px;
  color: var(--wf-muted);
}

/* Accessibility Utilities */
.sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  white-space: nowrap;
  border: 0;
}

