  color: var(--wf-bg);
  font-size: 12px;
  border-radius: var(--wf-radius);
  white-space: nowrap;
  z-index: 1000;
  pointer-events: none;
}

.wf-tooltip-top {
  bottom: 100%;
  left: 50%;
  transform: translateX(-50%);
  margin-bottom: 8px;
}

.wf-tooltip-bottom {
  top: 100%;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 8px;
}

.wf-tooltip-left {
  right: 100%;
  top: 50%;
  transform: translateY(-50%);
  margin-right: 8px;
}

.wf-tooltip-right {
  left: 100%;
  top: 50%;
  transform: translateY(-50%);
  margin-left: 8px;
}

.wf-popover {
  background: var(--wf-bg);
  border: 1px solid var(--wf-border);
  border-radius: var(--wf-radius);
  box-shadow: var(--wf-shadow-lg);
  min-width: 200px;
}

.wf-popover-header {
  padding: 12px 16px;
  border-bottom: 1px solid var(--wf-border);
  font-weight: 600;
}

.wf-popover-body {
  padding: 16px;
}

.wf-dropdown {
  background: var(--wf-bg);
  border: 1px solid var(--wf-border);
  border-radius: var(--wf-radius);
  box-shadow: var(--wf-shadow-md);
  min-width: 160px;
  padding: 4px 0;
}

.wf-dropdown-item {
  display: block;
  width: 100%;
  padding: 8px 16px;
  background: transparent;
  border: none;
  text-align: left;
  font-size: 14px;
  cursor: pointer;
}

.wf-dropdown-item:hover:not(:disabled) {
  background: rgba(0, 0, 0, 0.05);
}

.wf-dropdown-item-danger {
  color: var(--wf-danger);
}

.wf-dropdown-item-disabled {
  opacity: 0.5;
  cursor: not-allowed;
