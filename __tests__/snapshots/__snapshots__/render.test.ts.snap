  padding: 0;
  margin: 0;
}

.wf-list-ordered {
  list-style: decimal;
  padding-left: 24px;
}

.wf-list-none {
  list-style: none;
}

.wf-list-item {
  padding: 8px 0;
  border-bottom: 1px solid var(--wf-border);
}

.wf-list-item:last-child {
  border-bottom: none;
}

